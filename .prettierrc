{
	"trailingComma": "none",
	"tabWidth": 1,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES",
		"@/components/(.*)$",
		"@/hooks/(.*)$",
		"@/services/(.*)$",
		"@/assets/(.*)$",
		"@/providers/(.*)$",
		"@/types/(.*)$",
		"@../(.*)$",
		"@./(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
